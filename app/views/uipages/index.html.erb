
<h1>Listing users</h1>
 
<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Nome</th>
      <th>Idade</th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
    <% i = 0 %>
    <% @users.each do |user| %>
    <% i = i + 1 %>
      <tr>
        <th scope="row"><%= i %></th>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td>Bot√£o</td>
      </tr>
    <% end %>
  </tbody>
</table>


